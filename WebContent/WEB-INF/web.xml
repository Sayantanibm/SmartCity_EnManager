<?xml version="1.0" encoding="UTF-8"?>
<web-app id="WebApp_ID" version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
	<display-name>SmartCity_EnManager</display-name>
	<welcome-file-list>
		<welcome-file>welcome.jsp</welcome-file>
	</welcome-file-list>
	<servlet>
	<servlet-name>UserRegistrationServlet</servlet-name>
	<servlet-class>com.ibm.smartcity.servlet.UserRegistrationServlet</servlet-class>
	</servlet>
	<servlet-mapping>
	<servlet-name>UserRegistrationServlet</servlet-name>
	<url-pattern>/registerUser.do</url-pattern>
	</servlet-mapping>
	<servlet>
	<servlet-name>LoginServelet</servlet-name>
	<servlet-class>com.ibm.smartcity.servlet.LoginServelet</servlet-class>
	</servlet>
	<servlet-mapping>
	<servlet-name>LoginServelet</servlet-name>
	<url-pattern>/login.do</url-pattern>
	</servlet-mapping>
	<servlet>
	<servlet-name>EnvironmentIssueServlet</servlet-name>
	<servlet-class>com.ibm.smartcity.servlet.EnvironmentIssueServlet</servlet-class>
	</servlet>
	<servlet-mapping>
	<servlet-name>EnvironmentIssueServlet</servlet-name>
	<url-pattern>/reportIssue.do</url-pattern>
	</servlet-mapping>
	<servlet>
	<servlet-name>IssueListServlet</servlet-name>
	<servlet-class>com.ibm.smartcity.servlet.IssueListServlet</servlet-class>
	</servlet>
	<servlet-mapping>
	<servlet-name>IssueListServlet</servlet-name>
	<url-pattern>/issuelist.do</url-pattern>
	</servlet-mapping>
</web-app>